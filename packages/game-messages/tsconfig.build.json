{
	"extends": "./tsconfig.json",
	"compilerOptions": {
		"composite": true,
		"outDir": "dist",
		"rootDir": "src",
		"declaration": true,
		"declarationMap": true,
		"sourceMap": true,
		"module": "CommonJS",
		"moduleResolution": "node",
		"noEmit": false,
		"allowImportingTsExtensions": false
	},
	"include": ["src/**/*"]
}
