{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"skipLibCheck": true,
		"moduleDetection": "force",
		"esModuleInterop": true,
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noFallthroughCasesInSwitch": true,
		"noUncheckedSideEffectImports": true
	}
}
